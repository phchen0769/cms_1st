  "files.associations": {
    "*.vue": "vue"
  },
  "eslint.enable": true, //开启eslint检查
  "editor.codeActionsOnSave": {
    //使用eslint来fix，包括格式化会自动fix和代码质量检查会给出错误提示
    "source.fixAll.eslint": true
  }
}
